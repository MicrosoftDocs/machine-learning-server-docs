{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.svg",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "ms.author": "charlhe",
      "breadcrumb_path": "breadcrumb/toc.yml",
      "contributors_to_exclude": [
        "openpublishingbuild",
        "v-thepet",
        "sudeepku",
        "PRmerger"
      ],
      "searchScope": [
        "Machine Learning Server"
      ]
    },
    "fileMetadata": {},
    "template": [],
    "dest": "microsoft-r",
    "markdownEngineName": "markdig"
  }
}
